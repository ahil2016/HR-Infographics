<script>
// tenure range start
	// tenure range start
		var tenure16Val = '';
		var tenure11Val = '';
		var tenure6Val = '';
		var tenure3Val = '';
		var tenure1Val = '';
		var tenure0Val = '';

	$("#tenure16 .tenureval").on('DOMSubtreeModified', function() {
		var tenure16numwithcama=$(this).text();
		var tenure16numwithoutcama=tenure16numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure16numwithoutcama))) {
		tenure16Val = parseInt($(this).text());
		redrawTenure();
	    }
	})
	$("#tenure11 .tenureval").on('DOMSubtreeModified', function() {
		var tenure11numwithcama=$(this).text();
		var tenure11numwithoutcama=tenure11numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure11numwithoutcama))) {
		tenure11Val = parseInt(tenure11numwithoutcama);
		redrawTenure();
	    }
	})
	$("#tenure6 .tenureval").on('DOMSubtreeModified', function() {
		var tenure6numwithcama=$(this).text();
		var tenure6numwithoutcama=tenure6numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure6numwithoutcama))) {
		tenure6Val = parseInt(tenure6numwithoutcama);
		redrawTenure();	    
	     }
	})
	$("#tenure3 .tenureval").on('DOMSubtreeModified', function() {
		var tenure3numwithcama=$(this).text();
		var tenure3numwithoutcama=tenure3numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure3numwithoutcama))) {
		tenure3Val = parseInt(tenure3numwithoutcama);
		redrawTenure();
	    }
	})
	$("#tenure1 .tenureval").on('DOMSubtreeModified', function() {
		var tenure1numwithcama=$(this).text();
		var tenure1numwithoutcama=tenure1numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure1numwithoutcama))) {
		tenure1Val = parseInt(tenure1numwithoutcama);
		redrawTenure();
	    }
	})
	$("#tenure0 .tenureval").on('DOMSubtreeModified', function() {
		var tenure0numwithcama=$(this).text();
		var tenure0numwithoutcama=tenure0numwithcama.replace(",","");
	    if (!isNaN(parseInt(tenure0numwithoutcama))) {
		tenure0Val = parseInt(tenure0numwithoutcama);
		redrawTenure();
	    }
	})

	function redrawTenure() {

		if(!isNaN(tenure0Val) && !isNaN(tenure16Val) && !isNaN(tenure11Val) && !isNaN(tenure6Val) && !isNaN(tenure3Val) && !isNaN(tenure1Val)

) {

var maxTenureVal = Math.max(tenure0Val,tenure16Val,tenure11Val,tenure6Val, tenure3Val, tenure1Val);

	$("#tenure0 .tenurebar").css("width", ((tenure0Val/maxTenureVal) * 100) + '%');
$("#tenure16 .tenurebar").css("width", ((tenure16Val/maxTenureVal) * 100) + '%');
	$("#tenure11 .tenurebar").css("width", ((tenure11Val/maxTenureVal) * 100) + '%');
$("#tenure6 .tenurebar").css("width", ((tenure6Val/maxTenureVal) * 100) + '%');
$("#tenure3 .tenurebar").css("width", ((tenure3Val/maxTenureVal) * 100) + '%');
$("#tenure1 .tenurebar").css("width", ((tenure1Val/maxTenureVal) * 100) + '%');
	
		}

	}




// tenure range end
</script>




<script>
// diversity start
var tenureDiversityMale = 0;
var tenureDiversityFemale = 0;
	$("#femaleTenureVal .diver-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenureDiversityFemale = parseInt($(this).text());
		redrawTenureDiversity();
	    }
	})
	$("#maleTenureVal .diver-val").on('DOMSubtreeModified', function() {
	    if (!isNaN(parseInt($(this).text()))) {
		tenureDiversityMale = parseInt($(this).text());
		redrawTenureDiversity();
	    }
	})

function redrawTenureDiversity() {
	
	if(!isNaN(tenureDiversityMale) && !isNaN(tenureDiversityFemale)) {
		$("#femaleTenureVal .diversity-img-bg").css("height", tenureDiversityFemale + '%');
		$("#maleTenureVal .diversity-img-bg").css("height",  tenureDiversityMale + '%');
	}
}


// diversity end

</script>


